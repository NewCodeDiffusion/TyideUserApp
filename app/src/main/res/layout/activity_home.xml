<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Mapview.HomeActivity">
    <!--
        android:name="com.google.android.gms.maps.SupportMapFragment"
    -->
    <FrameLayout
        android:id="@+id/fraimMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="260dp"
            android:src="@drawable/current_location" />

        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:orientation="vertical">

            <androidx.core.widget.NestedScrollView
                android:id="@+id/bottomSheet1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:elevation="16dp"
                app:behavior_hideable="false"
                app:behavior_peekHeight="480dp"
                app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior"
                tools:ignore="MissingPrefix">

                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rectangle_whitetop_cure"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/rectangle_skybluetopcure"
                        android:orientation="vertical"
                        android:paddingBottom="15dp">

                        <ImageView
                            android:layout_width="44dp"
                            android:layout_height="3.5dp"
                            android:layout_gravity="center"
                            android:layout_marginTop="8.5dp"
                            android:src="@drawable/rectangle_skyblue" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="16.7dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginRight="16.7dp"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/sfpro_display_medium"
                                android:letterSpacing="0.0525"
                                android:text="@string/one_way_trip"
                                android:textColor="@color/skyblue"
                                android:textSize="16sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/sfpro_display_medium"
                                android:letterSpacing="0.0525"
                                android:text="@string/_5_ride_available"
                                android:textColor="@color/skyblue"
                                android:textSize="16sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        android:visibility="gone"
                        android:layout_gravity="center"
                        android:id="@+id/call"
                        android:padding="2dp"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerVertical="true"
                        app:tint="@color/black"
                        android:src="@drawable/ic_call_24"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="16dp"
                        android:orientation="horizontal">

                        <RadioGroup
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:weightSum="2">

                            <RadioButton
                                android:id="@+id/rb_Outstation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1"
                                android:checked="true"
                                android:text="Outstation"
                                android:textColor="@color/red" />

                            <RadioButton
                                android:id="@+id/rb_local"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1"
                                android:checked="true"
                                android:text="Local"
                                android:textColor="@color/red" />

                            <RadioButton
                                android:visibility="gone"
                                android:id="@+id/rb_Airport"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1"
                                android:checked="true"
                                android:text="Airport"
                                android:textColor="@color/red" />
                        </RadioGroup>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="6.5dp"
                        android:background="@color/white"
                        android:orientation="horizontal"
                        android:paddingBottom="2sp">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout

                                android:id="@+id/ll_selectPackage"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:orientation="horizontal">
                                <ImageView
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginTop="2.5dp"
                                    android:src="@drawable/ic_package_24" />

                                <LinearLayout

                                    android:layout_marginLeft="15dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="start"
                                        android:layout_gravity="start"
                                        android:text="Package"
                                        android:textColor="@color/red"
                                        android:textSize="15sp" />

                                    <TextView
                                        android:id="@+id/tv_SelectPackage"
                                        android:layout_width="match_parent"
                                        android:backgroundTint="@color/black"
                                        android:hint="Select Package"
                                        android:textColor="@color/low_orange"
                                        android:layout_height="wrap_content"
                                        android:dropDownSelector="@color/black"/>

                                   <androidx.recyclerview.widget.RecyclerView
                                       android:visibility="gone"
                                       android:id="@+id/rv_selectPackage"
                                       android:layout_width="match_parent"
                                       android:layout_height="wrap_content"
                                       app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                       android:orientation="vertical"/>


                                    <View

                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:layout_marginTop="6.5sp"
                                        android:background="@color/silver" />


                                </LinearLayout>

                            </LinearLayout>
                            <LinearLayout
                                android:visibility="gone"
                                android:id="@+id/ll_selectTripType"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1.2"
                                android:gravity="center"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_marginRight="10dp"
                                    android:layout_marginLeft="38dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical">

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="start"
                                        android:layout_gravity="start"
                                        android:text="Select Trip Type"
                                        android:textColor="@color/red"
                                        android:textSize="12sp" />

                                    <Spinner
                                        android:id="@+id/sp_SelectTripType"
                                        android:layout_width="match_parent"
                                        android:backgroundTint="@color/black"
                                        android:layout_height="wrap_content"
                                        android:dropDownSelector="@color/black"/>


                                </LinearLayout>
                                <View
                                    android:layout_marginLeft="38dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="1dp"
                                    android:layout_marginTop="6.5sp"
                                    android:background="@color/silver" />

                            </LinearLayout>
                            <LinearLayout
                                android:layout_marginTop="8dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_marginLeft="1dp"
                                    android:layout_marginTop="2.5dp"
                                    android:src="@drawable/ic_map_pin" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Start Location"
                                            android:textColor="@color/red"
                                            android:textSize="15dp" />

                                        <RelativeLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content">
                                            <AutoCompleteTextView
                                                android:textSize="14dp"
                                                android:background="@null"
                                                android:id="@+id/tv_userCurrentLocation"
                                                android:hint="Select Pickup Location"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"/>




                                            <ImageView
                                                android:id="@+id/iv_cross"
                                                android:layout_width="24dp"
                                                android:layout_height="24dp"
                                                android:layout_alignParentEnd="true"
                                                android:layout_marginRight="20dp"
                                                android:src="@drawable/ic_cross_24"
                                                android:visibility="gone" />
                                        </RelativeLayout>

                                    </LinearLayout>

                                    <View
                                        android:layout_width="match_parent"
                                        android:layout_height="1dp"
                                        android:layout_marginTop="6.5sp"
                                        android:background="@color/silver" />

                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:layout_marginTop="15sp"
                                android:layout_marginBottom="10sp"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="15dp"
                                    android:layout_height="15dp"
                                    android:layout_marginTop="2dp"
                                    android:src="@drawable/ic_baseline_add_location_24" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="End Location"
                                            android:textColor="@color/red"
                                            android:textSize="14sp" />

                                        <androidx.recyclerview.widget.RecyclerView
                                            android:visibility="gone"
                                            android:layout_marginTop="10dp"
                                            android:id="@+id/rv_SelectMultiCity"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                                            android:orientation="vertical"/>

                                        <RelativeLayout
                                            android:layout_marginTop="10dp"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content">

                                            <AutoCompleteTextView
                                                android:textSize="14dp"

                                                android:background="@null"
                                                android:id="@+id/tv_dropLocation"
                                                android:hint="Select Drop Location"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"/>
                                            <ImageView
                                                android:id="@+id/iv_crossdrop"
                                                android:layout_width="24dp"
                                                android:layout_height="24dp"
                                                android:layout_alignParentEnd="true"
                                                android:layout_marginRight="20dp"
                                                android:src="@drawable/ic_cross_24"
                                                android:visibility="gone" />
                                        </RelativeLayout>


                                    </LinearLayout>


                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>


                        <ImageView
                            android:layout_width="17dp"
                            android:layout_height="17dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="15dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginBottom="5dp"
                            android:src="@drawable/ic_baseline_compare_arrows_24"
                            android:visibility="gone" />


                    </LinearLayout>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:layout_marginBottom="15sp"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:id="@+id/ll_pickUpDate"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Pick Up Date"
                                            android:textColor="@color/red"
                                            android:textSize="11.7sp" />

                                        <TextView
                                            android:id="@+id/tv_PickupDate"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Select PickUp Date"
                                            android:textColor="@color/black"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="12:00 PM"
                                            android:textSize="13.3sp"
                                            android:visibility="gone" />

                                    </LinearLayout>
                                </LinearLayout>

                                <View
                                    android:layout_width="0.5dp"
                                    android:layout_height="match_parent"
                                    android:background="@color/dark_grey" />

                                <ImageView
                                    android:layout_width="17dp"
                                    android:layout_height="17dp"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginRight="20dp"
                                    android:layout_marginBottom="5dp"
                                    android:src="@drawable/ic_baseline_compare_arrows_24"
                                    android:visibility="gone" />

                                <LinearLayout
                                    android:visibility="gone"
                                    android:id="@+id/ll_dropDate"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Drop Date"
                                            android:textColor="@color/red"
                                            android:textSize="11.7sp" />

                                        <TextView
                                            android:id="@+id/tv_dropDate"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="Select Drop Date"
                                            android:textColor="@color/black"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:text="12:00 PM"
                                            android:textSize="13.3sp"
                                            android:visibility="gone" />

                                    </LinearLayout>
                                </LinearLayout>

                                <View
                                    android:layout_width="0.5dp"
                                    android:layout_height="match_parent"
                                    android:background="@color/dark_grey" />

                                <LinearLayout
                                    android:id="@+id/ll_pickupTime"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1.2"
                                    android:gravity="center"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="vertical">

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:layout_gravity="center"
                                            android:text="Pick Up Time"
                                            android:textColor="@color/red"
                                            android:textSize="12sp" />

                                        <Spinner
                                            android:id="@+id/sp_SelectCurrentTime"
                                            android:layout_width="match_parent"
                                            android:backgroundTint="@color/black"
                                            android:layout_height="wrap_content"
                                            android:dropDownSelector="@color/black"/>


                                    </LinearLayout>
                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>
                    <LinearLayout
                        android:layout_marginTop="16dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.2"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_marginRight="10dp"
                                android:layout_marginLeft="10dp"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:layout_gravity="start"
                                    android:text="Select Car Type"
                                    android:textColor="@color/red"
                                    android:textSize="12sp" />

                                <Spinner
                                    android:id="@+id/sp_SelectCarType"
                                    android:layout_width="match_parent"
                                    android:backgroundTint="@color/black"
                                    android:layout_height="wrap_content"
                                    android:dropDownSelector="@color/black"/>


                            </LinearLayout>

                            <View
                                android:layout_width="0.5dp"
                                android:layout_height="match_parent"
                                android:background="@color/dark_grey" />


                            <LinearLayout
                                android:layout_marginRight="10dp"
                                android:layout_marginLeft="10dp"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:layout_gravity="start"
                                    android:text="Select Model Type"
                                    android:textColor="@color/red"
                                    android:textSize="12sp" />

                                <Spinner
                                    android:id="@+id/sp_SelectCarModel"
                                    android:layout_width="match_parent"
                                    android:backgroundTint="@color/black"
                                    android:layout_height="wrap_content"
                                    android:dropDownSelector="@color/black"/>


                            </LinearLayout>
                        </LinearLayout>


                        <View

                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="6.5sp"
                            android:background="@color/silver" />

                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_SearchCar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/gradient_orange"
                        android:elevation="0.8dp"
                        android:fontFamily="@font/sfpro_display_medium"
                        android:letterSpacing="0.0525"
                        android:paddingTop="15dp"
                        android:paddingBottom="15dp"
                        android:text="Submit"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="60dp"
                        android:layout_marginRight="60dp"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="12sp" />
                </LinearLayout>
            </androidx.core.widget.NestedScrollView>

            <fragment
                android:id="@+id/map"
                android:name="com.google.android.gms.maps.SupportMapFragment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp">

                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/ll_trip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/rectangle_whiterounded"
                        android:elevation="5dp"
                        android:orientation="horizontal">


                        <LinearLayout
                            android:id="@+id/li_oneway"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/rectangle_bluecornnor"
                            android:orientation="vertical"
                            android:paddingLeft="23dp"
                            android:paddingTop="13dp"
                            android:paddingRight="23dp"
                            android:paddingBottom="13dp">

                            <TextView
                                android:id="@+id/oneway"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/one_way"
                                android:textColor="#ffffff"
                                android:textSize="13.3sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_roundtrip"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:paddingLeft="15dp"
                            android:paddingTop="13dp"
                            android:paddingRight="23dp"
                            android:paddingBottom="13dp">

                            <TextView
                                android:id="@+id/roundtrip"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/round_trip"
                                android:textColor="#919ca5"
                                android:textSize="13.3sp" />
                        </LinearLayout>

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/sideMenu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15.3dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/side_menu" />





                </FrameLayout>
            </LinearLayout>


        </androidx.coordinatorlayout.widget.CoordinatorLayout>
    </FrameLayout>

    <include layout="@layout/drawer_slidemenu_gr" />
</androidx.drawerlayout.widget.DrawerLayout>